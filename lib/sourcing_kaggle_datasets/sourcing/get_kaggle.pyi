from typing import Any

class Kaggle:
    api: Any
    def __init__(self) -> None: ...
    split_ds: Any
    owner: Any
    dataset_name: Any
    path: Any
    filepath: Any
    def create_dataset_dir(self, dataset, path): ...
    human_readable_size: Any
    def parse_size(self, human_readable_size: str = ...): ...

class Datasets(Kaggle):
    def list_datasets(self, search: str = ..., tag_ids: str = ..., file_type: str = ..., user: str = ..., mine: bool = ..., license_name: str = ..., sort_by: str = ..., min_size: str = ..., max_size: str = ..., page: int = ...): ...
    def list_dataset_files(self, dataset): ...
    def get_dataset_files(self, dataset, path: str = ...): ...
    def get_dataset_file(self, dataset, file_name, path: str = ...): ...
